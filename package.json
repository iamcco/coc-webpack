{
  "name": "coc-webpack",
  "version": "1.0.0",
  "description": "autocomplete support for webpack config",
  "keywords": [
    "coc.nvim",
    "webpack"
  ],
  "engines": {
    "coc": "^0.0.73"
  },
  "main": "./out/index.js",
  "repository": "https://github.com/iamcco/coc-webpack",
  "author": "iamcco <ooiss@qq.com>",
  "license": "MIT",
  "contributes": {
    "configuration": {
      "title": "coc-webpack config",
      "properties": {
        "coc-webpack.enable": {
          "type": "boolean",
          "default": true,
          "description": "enable coc-webpack"
        },
        "coc-webpack.trace.server": {
          "type": "string",
          "default": "off",
          "enum": [
            "off",
            "messages",
            "verbose"
          ],
          "description": "Trace level of coc-webpack"
        }
      }
    }
  },
  "scripts": {
    "build": "tsc -p ./"
  },
  "activationEvents": [
    "onLanguage:javascript"
  ],
  "dependencies": {
    "typescript": "^3.5.3"
  },
  "devDependencies": {
    "@types/node": "^12.7.2",
    "coc.nvim": "^0.0.73",
    "vscode-languageserver-protocol": "^3.14.1"
  }
}
